    <script>
        // Get elements
        const aboutUsLink = document.getElementById('aboutUsLink');
        const aboutUsModal = document.getElementById('about-us-modal');
        const closeModal = document.querySelector('.modal-close');

        // Show modal when "About Us" link is clicked
        aboutUsLink.addEventListener('click', function (e) {
            e.preventDefault(); // Prevent default link behavior
            aboutUsModal.style.display = 'flex'; // Show the modal
        });

        // Close modal when close button is clicked
        closeModal.addEventListener('click', function () {
            aboutUsModal.style.display = 'none'; // Hide
        });

        // Close modal when clicking outside the modal content
        window.addEventListener('click', function (e) {
            if (e.target === aboutUsModal) {
                aboutUsModal.style.display = 'none'; // Hide
            }
        });
    </script>
</body>
